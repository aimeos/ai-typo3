{
	"name": "aimeos/ai-typo3",
	"description": "TYPO3 adapter for Aimeos web shops and e-commerce solutions",
	"keywords": ["aimeos", "adapter", "typo3", "cms", "shop", "e-commerce"],
	"homepage": "https://aimeos.org/TYPO3",
	"type": "aimeos-extension",
	"license": "LGPL-3.0-or-later",
	"support": {
		"source": "https://github.com/Aimeos/ai-typo3",
		"issues": "https://github.com/Aimeos/ai-typo3/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": "~7.1",
		"composer/installers": "^1.0",
		"zendframework/zend-diactoros": "~1.0",
		"aimeos/aimeos-core": "dev-master"
	},
	"require-dev": {
		"typo3/cms-core": "~8.7||~9.5||~10.1",
		"phpunit/phpunit": "~7.0||~8.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\MShop\\": "lib/custom/src/MShop",
			"Aimeos\\MW\\": "lib/custom/src/MW"
		},
		"classmap": [
			"lib/custom/src"
		]
	}
}
