{
	"name": "aimeos/ai-typo3",
	"description": "TYPO3 adapter for Aimeos e-commerce integration",
	"keywords": ["aimeos", "adapter", "typo3", "cms", "shop", "e-commerce"],
	"homepage": "https://aimeos.org/TYPO3",
	"type": "aimeos-extension",
	"license": "LGPL-3.0-or-later",
	"support": {
		"source": "https://github.com/Aimeos/ai-typo3",
		"issues": "https://github.com/Aimeos/ai-typo3/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": "~7.1||~8.0",
		"ext-fileinfo": "*",
		"nyholm/psr7": "~1.2",
		"nyholm/psr7-server": "~1.0",
		"aimeos/aimeos-core": "~2022.04"
	},
	"require-dev": {
		"typo3/cms-core": "~9.5||~10.3",
		"typo3/cms-extbase": "~9.5||~10.3",
		"phpunit/phpunit": "~7.0||~8.0||~9.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\": "src"
		},
		"classmap": [
			"src"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Aimeos\\": "tests"
		},
		"classmap": [
			"tests"
		]
	}
}
