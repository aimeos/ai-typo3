namespace TYPO3\CMS\Core\Authentication;


class AbstractUserAuthentication
{
	private $data = [];

	public function getSessionData( $key )
	{
		if ( isset($this->data[$key]) ) {
			return $this->data[$key];
		}
	}

	public function setAndSaveSessionData( $key, $data )
	{
		$this->data[$key] = $data;
	}
}
